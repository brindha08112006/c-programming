#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char ch;

    scanf("%c", &ch);

    // Check if it is an alphabet

    if (isalpha(ch)) {

        // Convert to uppercase to simplify checking

        char upper = toupper(ch);

        if (upper == 'A' || upper == 'E' || upper == 'I' || upper == 'O' || upper == 'U') {

            printf("The Character %c is Vowel\n", ch);

        } else {

            printf("The Character %c is Consonant\n", ch);

        }

    } else {

        printf("Invalid Input\n");

    }

    return 0;

}
