#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a, b;
    char op;
    float result;

    scanf("%d", &a);
    scanf("%d", &b);
    scanf(" %c", &op); 
    if (op == '+') {

        result = a + b;
        printf("Addition of two number is %.1f\n", result);

    } else if (op == '-') {

        result = a - b;
        printf("Subtraction of two number is %.1f\n", result);

    } else if (op == '*') {
        result = a * b;

        printf("Multiplication of two number is %.1f\n", result);
    } else if (op == '/') {

        result = (float)a / b;
        printf("Division of two number is %.1f\n", result);

    } else {
        printf("Invalid Input\n");

    }

    return 0;

}

