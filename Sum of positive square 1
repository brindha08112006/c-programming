#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int n, sum=0;
    scanf("%d", &n);
    int arr[n];
    for(int i=0;i<n;i++)
        {
        scanf("%d", &arr[i]);
    }
    for(int i=0;i<n;i++)
    {
        if (arr[i]>0)
            {
            sum += arr[i] * arr[i];
        }
    }
    printf("%d", sum);    
    return 0;
}
